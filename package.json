    "cross-env": "^3.2.3",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.1.1",
    "laravel-mix": "0.*",
    "less-loader": "^4.0.4",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "redux-devtools": "^3.4.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "vue": "^2.1.10",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
